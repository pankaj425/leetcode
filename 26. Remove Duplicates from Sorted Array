class Solution {
    public int removeDuplicates(int[] nums) {
        int count=1,temp=nums[0];
        for(int i=0;i<nums.length;i++)
        {
             if(nums[i]!=temp)
             {
                temp=nums[i];
                nums[count]=nums[i];
                count++;
             }

        }

        return count;
    
    }
}
